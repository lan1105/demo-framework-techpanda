<?xml version="1.0" encoding="UTF-8"?>
<suite parallel="false" name="JQuery">

	<listeners>
		<listener class-name="commons.MethodListener" />
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
	</listeners>

	<parameter name="jQueryUrl" value="https://blueimp.github.io/jQuery-File-Upload/" />

	<test name="Run on FIREFOX browser">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.jquery.upload.Level_11_Upload_Files" />
		</classes>
	</test>

</suite> 